/**
 * Or gate:
 * if (a or b) out = 1, else out = 0 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out =ax);
    Not(in=b, out = bx);

    Nand(a=ax, b=bx, out=out);
}
